{
    "name": "covex-nn/docker-symfony-se",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" for docker distribution",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [ "/composer/vendor/symfony/var-dumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": "^7.0.8",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5.11",
        "friendsofsymfony/user-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^5.0",
        "sonata-project/admin-bundle": "^3.24",
        "sonata-project/doctrine-orm-admin-bundle": "^3.1",
        "symfony/asset": "^3.4",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "symfony/debug-bundle": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/stopwatch": "^3.4",
        "symfony/web-profiler-bundle": "^3.4"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
